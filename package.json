
{
  "name": "psd-to-sprite-atlas",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "main": "main.cjs",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "electron:dev": "vite & electron .",
    "electron:build": "npm run build && electron-builder"
  },
  "build": {
    "appId": "com.psdtool.atlas",
    "productName": "PSD图集转换助手",
    "directories": {
      "output": "dist_desktop"
    },
    "files": [
      "dist/**/*",
      "main.cjs"
    ],
    "win": {
      "target": "portable",
      "icon": "public/icon.png"
    },
    "mac": {
      "target": "dmg",
      "category": "public.app-category.developer-tools"
    }
  },
  "dependencies": {
    "@google/genai": "^1.34.0",
    "ag-psd": "^29.0.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3"
  },
  "devDependencies": {
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "electron": "^31.0.0",
    "electron-builder": "^24.13.3",
    "typescript": "^5.0.0",
    "vite": "^6.0.0",
    "@vitejs/plugin-react": "^4.3.0",
    "autoprefixer": "^10.4.0",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.4.0"
  }
}
